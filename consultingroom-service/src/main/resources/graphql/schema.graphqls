type Consultorio {
    id_consultorio: ID!
    numero_consultorio: String
    tipo: String
    estado: String
    sede: String
    ciudad: String
}

type Query {
    getAllConsultorios: [Consultorio]
    getConsultorioById(id: ID!): Consultorio
}

type TipoConsultorio {
    id: ID!
    nombre: String
}

type Sede {
    id: ID!
    nombre: String
}

type EstadoConsultorio {
    id: ID!
    nombre: String
}
